{
  "name": "@reisetra/soda",
  "version": "1.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --path tsconfig.build.json --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch --webpack --path tsconfig.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop ecosystem.config.js --env production",
    "deploy:prod": "pm2 reload ecosystem.config.js --env production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"./**/*.{js,ts,json}\"",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_PATH=./src jest --config ./test/jest-e2e.json --forceExit",
    "migrate": "prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@nestjs/common": "7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/throttler": "^1.1.3",
    "@prisma/client": "2.21.2",
    "argon2": "^0.27.2",
    "aws-sdk": "^2.897.0",
    "cache-manager": "^3.4.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "cuid": "^2.1.8",
    "google-auth-library": "^7.0.4",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "logzio-nodejs": "^2.0.2",
    "nanoid": "^3.1.22",
    "nestjs-pino": "^1.4.0",
    "nodemailer": "^6.6.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-pretty": "^4.7.1",
    "ramda": "^0.27.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "4.17.11",
    "@types/hbs": "^4.0.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "14.14.43",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/ramda": "^0.27.40",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma": "^2.25.0",
    "supertest": "6.1.3",
    "ts-jest": "26.5.5",
    "ts-loader": "9.1.1",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
